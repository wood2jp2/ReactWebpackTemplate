{
  "name": "reactwebpacktemplate",
  "version": "1.0.0",
  "description": "template engine for React / webpack projects with Node",
  "main": "./src/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "push": "git add && git commit -m 'added class stuff' && git push",
    "serve": "live-server public/",
    "babel": "babel src/app.js --out-file=public/scripts/app.js --presets=env,react --watch",
    "build": "webpack --watch"
  },
  "author": "JPW",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "live-server": "^1.2.0",
    "webpack": "^3.1.0"
  }
}
